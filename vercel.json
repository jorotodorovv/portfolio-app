{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "env": {
      "PUBLIC_API_URL": "@vercel-env-vars"
    },
    "build": {
      "env": {
        "PUBLIC_API_URL": "[[ $VERCEL_GIT_COMMIT_REF == 'main' ? 'https://portfolio-app-jorotodorovvs-projects.vercel.app/' : ( $VERCEL_GIT_COMMIT_REF == 'develop' ? 'https://develop--my-portfolio-app.vercel.app' : 'https://portfolio-app-git-' + $VERCEL_GIT_COMMIT_REF + '-jorotodorovvs-projects.vercel.app' ) ]]"
      }
    }
  }